{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.inlineSuggest.enabled": true,
    "editor.formatOnSave": true,
    "editor.rulers": [
        120
    ],
    "editor.colorDecorators": true
}